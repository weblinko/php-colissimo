{
    "name": "weblinko/php-colissimo",
    "description": "Package generated from https://ws.colissimo.fr/sls-ws/SlsServiceWS/2.0?wsdl",
    "require": {
        "php": ">=7.4",
        "ext-dom": "*",
        "ext-mbstring": "*",
        "ext-soap": "*",
        "wsdltophp/packagebase": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Colissimo\\": "./src/",
            "SoapClient\\": "./SoapClient/"
        }
    },
    "require-dev": {
      "phpstan/phpstan": "^1.4"
    },
    "scripts": {
      "phpstan": "vendor/bin/phpstan analyze src --level=3"
    },
    "type": "library",
    "license": "MIT"
}
